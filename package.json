{
  "name": "find-es-resources",
  "version": "1.0.1",
  "author": "Brett Zamir <brettz9@yahoo.com>",
  "contributors": [],
  "description": "Find resource files within JavaScript files",
  "license": "MIT",
  "keywords": [
    "serviceworker",
    "service-worker"
  ],
  "type": "module",
  "bin": {
    "find-es-resources": "./bin/index.js"
  },
  "main": "src/index.js",
  "exports": "./src/index.js",
  "browserslist": [
    "cover 100%"
  ],
  "scripts": {
    "prepublishOnly": "pnpm i",
    "help": "./bin/index.js -h",
    "v": "./bin/index.js -v",
    "example": "./bin/index.js -i ./src/index.js -o output.js",
    "example-json": "./bin/index.js -i ./src/index.js -o output.json",
    "build-cli-svg": "clp --format=svg --config=src/optionDefinitions.js doc-includes/cli.svg",
    "build-cli": "npm run build-cli-svg",
    "eslint": "eslint --ext=js,cjs,md,html .",
    "lint": "npm run eslint",
    "mocha": "mocha --require chai/register-expect test/*.js",
    "test": "c8 npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/find-es-resources.git"
  },
  "bugs": {
    "url": "https://github.com/brettz9/find-es-resources/issues"
  },
  "homepage": "https://github.com/brettz9/find-es-resources",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "command-line-basics": "^1.0.1",
    "es-file-traverse": "^0.11.0",
    "esquery": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/eslint-parser": "^7.14.5",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@brettz9/eslint-plugin": "^1.0.3",
    "c8": "^7.7.2",
    "chai": "^4.3.4",
    "command-line-publish": "^1.0.0",
    "eslint": "^7.28.0",
    "eslint-config-ash-nazg": "29.17.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.1.3",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-unicorn": "^33.0.1",
    "mocha": "^9.0.0",
    "nyc": "^15.1.0"
  }
}
